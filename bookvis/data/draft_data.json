 follow us.'}, {'entities': [{'type': 'x', 'word': '"'}, {'word': 'There'}, {'word': 'is'}, {'word': 'a'}, {'word': 'lady'}, {'type': 'x', 'word': ','}, {'word': 'it'}, {'word': 'seems'}, {'type': 'x', 'word': ','}, {'word': 'a'}, {'type': 'NE', 'word': 'Mrs.'}, {'type': 'NE', 'word': 'Younge'}, {'type': 'x', 'word': ','}, {'word': 'who'}, {'word': 'was'}, {'word': 'some'}, {'word': 'time'}, {'word': 'ago'}, {'word': 'governess'}, {'word': 'to'}, {'type': 'NE', 'word': 'Miss Darcy'}, {'type': 'x', 'word': ','}, {'word': 'and'}, {'word': 'was'}, {'word': 'dismissed'}, {'word': 'from'}, {'word': 'her'}, {'word': 'charge'}, {'word': 'on'}, {'word': 'some'}, {'word': 'cause'}, {'word': 'of'}, {'word': 'disapprobation'}, {'type': 'x', 'word': ','}, {'word': 'though'}, {'word': 'he'}, {'word': 'did'}, {'word': 'not'}, {'word': 'say'}, {'word': 'what'}, {'type': 'x', 'word': '.'}, {'word': 'She'}, {'word': 'then'}, {'word': 'took'}, {'word': 'a'}, {'word': 'large'}, {'word': 'house'}, {'word': 'in'}, {'type': 'NE', 'word': 'Edward-street'}, {'type': 'x', 'word': ','}, {'word': 'and'}, {'word': 'has'}, {'word': 'since'}, {'word': 'maintained'}, {'word': 'herself'}, {'word': 'by'}, {'word': 'letting'}, {'word': 'lodgings'}, {'type': 'x', 'word': '.'}, {'word': 'This'}, {'type': 'NE', 'word': 'Mrs.'}, {'type': 'NE', 'word': 'Younge'}, {'word': 'was'}, {'type': 'x', 'word': ','}, {'word': 'he'}, {'word': 'knew'}, {'type': 'x', 'word': ','}, {'word': 'intimately'}, {'word': 'acquainted'}, {'word': 'with'}, {'type': 'NE', 'word': 'Wickham'}, {'type': 'x', 'word': ';'}, {'word': 'and'}, {'word': 'he'}, {'word': 'went'}, {'word': 'to'}, {'word': 'her'}, {'word': 'for'}, {'word': 'intelligence'}, {'word': 'of'}, {'word': 'him'}, {'word': 'as'}, {'word': 'soon'}, {'word': 'as'}, {'word': 'he'}, {'word': 'got'}, {'word': 'to'}, {'word': 'town'}, {'type': 'x', 'word': '.'}, {'word': 'But'}, {'word': 'it'}, {'word': 'was'}, {'type': 'x', 'word': 'two'}, {'word': 'or'}, {'type': 'x', 'w