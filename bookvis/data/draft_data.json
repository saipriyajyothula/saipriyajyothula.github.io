ities': [{'word': 'At'}, {'word': 'length'}, {'type': 'x', 'word': ','}, {'word': 'however'}, {'type': 'x', 'word': ','}, {'word': 'the'}, {'word': 'remarks'}, {'word': 'of'}, {'word': 'her'}, {'word': 'companions'}, {'word': 'on'}, {'word': 'her'}, {'word': 'absence'}, {'word': 'of'}, {'word': 'mind'}, {'word': 'aroused'}, {'word': 'her'}, {'type': 'x', 'word': ','}, {'word': 'and'}, {'word': 'she'}, {'word': 'felt'}, {'word': 'the'}, {'word': 'necessity'}, {'word': 'of'}, {'word': 'appearing'}, {'word': 'more'}, {'word': 'like'}, {'word': 'herself'}, {'type': 'x', 'word': '.'}], 'paragraph': 56, 'original': 'At length, however, the remarks of her companions on her absence of mind aroused her, and she felt the necessity of appearing more like herself.'}, {'entities': [{'word': 'They'}, {'word': 'entered'}, {'word': 'the'}, {'word': 'woods'}, {'type': 'x', 'word': ','}, {'word': 'and'}, {'word': 'bidding'}, {'word': 'adieu'}, {'word': 'to'}, {'word': 'the'}, {'word': 'river'}, {'word': 'for'}, {'word': 'a'}, {'word': 'while'}, {'type': 'x', 'word': ','}, {'word': 'ascended'}, {'word': 'some'}, {'word': 'of'}, {'word': 'the'}, {'word': 'higher'}, {'word': 'grounds'}, {'type': 'x', 'word': ';'}, {'word': 'when'}, {'type': 'x', 'word': ','}, {'word': 'in'}, {'word': 'spots'}, {'word': 'where'}, {'word': 'the'}, {'word': 'opening'}, {'word': 'of'}, {'word': 'the'}, {'word': 'trees'}, {'word': 'gave'}, {'word': 'the'}, {'word': 'eye'}, {'word': 'power'}, {'word': 'to'}, {'word': 'wander'}, {'type': 'x', 'word': ','}, {'word': 'were'}, {'word': 'many'}, {'word': 'charming'}, {'word': 'views'}, {'word': 'of'}, {'word': 'the'}, {'word': 'valley'}, {'type': 'x', 'word': ','}, {'word': 'the'}, {'word': 'opposite'}, {'word': 'hills'}, {'type': 'x', 'word': ','}, {'word': 'with'}, {'word': 'the'}, {'word': 'long'}, {'word': 'range'}, {'word': 'of'}, {'word': 'woods'}, {'word': 'overspreading'}, {'word': 'many'}, {'type': 'x', 'word': ','}, {'word': 'and'}, {'word': 'occasionall