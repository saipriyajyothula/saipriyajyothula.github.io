ng'}, {'type': 'NE', 'word': 'Lady Catherine'}, {'type': 'x', 'word': "'s"}, {'word': 'expressions'}, {'type': 'x', 'word': ','}, {'word': 'however'}, {'type': 'x', 'word': ','}, {'word': 'she'}, {'word': 'could'}, {'word': 'not'}, {'word': 'help'}, {'word': 'feeling'}, {'word': 'some'}, {'word': 'uneasiness'}, {'word': 'as'}, {'word': 'to'}, {'word': 'the'}, {'word': 'possible'}, {'word': 'consequence'}, {'word': 'of'}, {'word': 'her'}, {'word': 'persisting'}, {'word': 'in'}, {'word': 'this'}, {'word': 'interference'}, {'type': 'x', 'word': '.'}, {'word': 'From'}, {'word': 'what'}, {'word': 'she'}, {'word': 'had'}, {'word': 'said'}, {'word': 'of'}, {'word': 'her'}, {'word': 'resolution'}, {'word': 'to'}, {'word': 'prevent'}, {'word': 'their'}, {'word': 'marriage'}, {'type': 'x', 'word': ','}, {'word': 'it'}, {'word': 'occurred'}, {'word': 'to'}, {'word': 'Elizabeth'}, {'word': 'that'}, {'word': 'she'}, {'word': 'must'}, {'word': 'meditate'}, {'word': 'an'}, {'word': 'application'}, {'word': 'to'}, {'word': 'her'}, {'word': 'nephew'}, {'type': 'x', 'word': ';'}, {'word': 'and'}, {'word': 'how'}, {'font': 'i', 'word': 'he'}, {'word': 'might'}, {'word': 'take'}, {'word': 'a'}, {'word': 'similar'}, {'word': 'representation'}, {'word': 'of'}, {'word': 'the'}, {'word': 'evils'}, {'word': 'attached'}, {'word': 'to'}, {'word': 'a'}, {'word': 'connection'}, {'word': 'with'}, {'word': 'her'}, {'type': 'x', 'word': ','}, {'word': 'she'}, {'word': 'dared'}, {'word': 'not'}, {'word': 'pronounce'}, {'type': 'x', 'word': '.'}, {'word': 'She'}, {'word': 'knew'}, {'word': 'not'}, {'word': 'the'}, {'word': 'exact'}, {'word': 'degree'}, {'word': 'of'}, {'word': 'his'}, {'word': 'affection'}, {'word': 'for'}, {'word': 'his'}, {'word': 'aunt'}, {'type': 'x', 'word': ','}, {'word': 'or'}, {'word': 'his'}, {'word': 'dependence'}, {'word': 'on'}, {'word': 'her'}, {'word': 'judgment'}, {'type': 'x', 'word': ','}, {'word': 'but'}, {'word': 'it'}, {'word': 'was'}, {'word': 'natural'}, {'word